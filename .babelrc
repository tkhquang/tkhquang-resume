{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": 16
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    [
      "file-loader",
      {
        "name": "[name].[ext]",
        "extensions": ["ttf"],
        "publicPath": "src/assets/fonts",
        "outputPath": "../../dist/assets/fonts",
        "context": "",
        "limit": 0
      },
      "fonts"
    ],
    [
      "file-loader",
      {
        "name": "[name].[ext]",
        "extensions": ["png"],
        "publicPath": "src/assets/images",
        "outputPath": "../../dist/assets/images",
        "context": "",
        "limit": 0
      },
      "images"
    ]
  ]
}
